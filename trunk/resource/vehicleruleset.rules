[low-fuel-risk-rule: (?v vehiclens:has-fuel-amount ?a), equal(?a, 0) -> (?v vehiclens:low-fuel-risk 'true')]
[med-fuel-risk-rule: (?v vehiclens:has-fuel-amount ?a), equal(?a, 50) -> (?v vehiclens:med-fuel-risk 'true')]
[high-fuel-risk-rule: (?v vehiclens:has-fuel-capacity ?c), (?v vehiclens:has-fuel-amount ?a), equal(?c, ?a)  -> (?v vehiclens:high-fuel-risk 'true')]
[low-fuel-risk-mitigation-rule: (?v vehiclens:low-fuel-risk 'true') -> (?v vehiclens:reload-fuel-mitigation 'true')]
[high-fuel-risk-mitigation-rule: (?v vehiclens:high-fuel-risk 'true') -> (?v vehiclens:unload-fuel-mitigation 'true')]
[low-fuel-risk-mitigation-action-rule: (?v vehiclens:reload-fuel-mitigation 'true') -> (?v vehiclens:reload-fuel-action 'true')]
[high-fuel-risk-mitigation-action-rule: (?v vehiclens:unload-fuel-mitigation 'true') -> (?v vehiclens:unload-fuel-action 'true')]
[reload-keep-calm-rule: (?v vehiclens:reload-fuel-mitigation 'true') -> (?v vehiclens:keep-calm-and-quiet 'true')]
[unload-keep-calm-rule: (?v vehiclens:unload-fuel-mitigation 'true') -> (?v vehiclens:keep-calm-and-quiet 'true')]
[weather-risk-rule: (?v airportns:has-weather ?w), notEqual(?w, 'clear') -> (?v airportns:weather-risk ?w)]
[visibility-risk-rule: (?a airportns:weather-risk ?w), (?v vehiclens:at-airport ?a) -> (?v vehiclens:visibility-risk ?v)]
[slow-mitigation-visibility-rule: (?v vehiclens:visibility-risk ?v) -> (?v vehiclens:slow-plane-mitigation ?v)]
[stop-mitigation-visibility-rule: (?v vehiclens:visibility-risk ?v) -> (?v vehiclens:stop-plane-mitigation ?v)]
[stop-action-rule: (?v vehiclens:stop-plane-mitigation ?v) -> (?v vehiclens:tell-plane-to-stop-action ?v)]
[slow-action-rule: (?v vehiclens:slow-plane-mitigation ?v) -> (?v vehiclens:tell-plane-to-slow-action ?v)]
[collision-exception: (?v vehiclens:has-speed 0.0), (?v1 vehiclens:has-speed 0.0) -> (?v vehiclens:wont-collide ?v1)]
[collision-risk-rule: (?v vehiclens:has-direction ?d), (?v1 vehiclens:has-direction ?e), sum(?d, 180.0, ?s), equal(?s, ?e), (?v vehiclens:on-way ?w), (?v1 vehiclens:on-way ?w), noValue(?v, vehiclens:wont-collide) -> (?v vehiclens:collision-risk ?v1)]
[move-plane-mitigation-rule: (?v vehiclens:collision-risk ?v1) -> (?v vehiclens:move-plane-mitigation ?v), (?v1 vehiclens:move-plane-mitigation ?v1)]
[slow-mitigation-collision-rule: (?v vehiclens:collision-risk ?v1) -> (?v vehiclens:slow-plane-mitigation ?v), (?v1 vehiclens:slow-plane-mitigation ?v1)]
[stop-mitigation-collision-rule: (?v vehiclens:collision-risk ?v1) -> (?v vehiclens:stop-plane-mitigation ?v), (?v1 vehiclens:stop-plane-mitigation ?v1)]
[bail-action-rule: (?v vehiclens:move-plane-mitigation ?v) -> (?v vehiclens:tell-plane-to-bail-left-action ?v), (?v vehiclens:tell-plane-to-bail-right-action ?v)]